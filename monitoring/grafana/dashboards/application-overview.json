{
  "dashboard": {
    "title": "Legal AI System - Application Overview",
    "tags": ["legal-ai", "application", "overview"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (RPS)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "requests/sec", "format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Response Time (P95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{status_code=~\"5..\"}[5m])",
            "legendFormat": "{{method}} {{endpoint}} ({{status_code}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "errors/sec", "format": "short"},
          {"format": "short"}
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ]
        }
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "http_requests_in_progress",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "requests", "format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Database Query Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(db_queries_total[5m])",
            "legendFormat": "{{operation}} {{table}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "queries/sec", "format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Database Query Duration (P95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{operation}} {{table}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "{{cache_type}} hit rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "hit rate %", "format": "percent"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "AI API Costs (Hourly)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(ai_cost_dollars_total[1h]) * 3600",
            "legendFormat": "{{provider}} {{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "$/hour", "format": "currencyUSD"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "System CPU Usage",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 32, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "System Memory Usage",
        "type": "gauge",
        "gridPos": {"x": 6, "y": 32, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "DB Connection Pool",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 32, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "db_connection_pool_checked_out / db_connection_pool_size * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"x": 18, "y": 32, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "active_users_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ]
  }
}
